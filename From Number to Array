#include<iostream>
#include<vector>
using namespace std;
int main(int argc, char const *argv[])
{
    int num;
    cin >> num;
    vector<int> arr;
    
    while(num){
        int element = num % 10;
        arr.push_back(element);
        num /= 10;
    }
    for(int i = arr.size() - 1; i >= 0; i--){
        cout << arr[i] << " ";
    }
    return 0;
}
